{
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "host.docker.internal",
      "Port": 8500,
      "Type": "Consul"
    }
  },
  "Aggregates": [
    {
      "RouteKeys": [
        "register-user",
        "save-person"
      ],
      "UpstreamPathTemplate": "/swagger-detail",
      "UpstreamHttpMethod": [ "Post" ],
      "Aggregator": "SaveUserAggregator"
    }
  ]
}